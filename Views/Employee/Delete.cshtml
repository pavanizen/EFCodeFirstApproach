
@model EFCodeFirstApproach.Models.Employee
@{
    ViewBag.Title = "Delete";
}

<h2>Delete Employee</h2>




<form action="/Employee/Delete/@Model.Id" method="post">
    <div class="form-row">
        <div class="form-group col-md-6">
            @Html.LabelFor(m => m.EmpName)

            @Html.HiddenFor(m => m.Id)
            @Model.EmpName
        </div>
        <div class="form-group col-md-6">
            @Html.LabelFor(m => m.Salary)
            @Model.Salary
        </div>

        <div class="form-group col-md-6">
            @Html.LabelFor(m => m.ManagerId)
            @Model.ManagerId
        </div>
        <div class="form-group col-md-6">
            @Html.LabelFor(m => m.DeptId)
            @Model.DeptId

        </div>
    </div>
    <button type="submit" class="btn btn-danger">Delete Employee</button>
    <a class="btn btn-dark" href="/Employee/Index">Cancel</a>
</form>

